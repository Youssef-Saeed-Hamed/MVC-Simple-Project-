@model UsersVM

<div class="form-group w-50">
	<label class="col-form-label" asp-for="Id">Id</label>
	<input readonly class="form-control" asp-for="Id" />
	<span asp-validation-for="Id"></span>
</div>
<div class="form-group w-50">
	<label class="col-form-label" asp-for="FirstName">FirstName</label>
	<input readonly type="type" class="form-control" asp-for="FirstName" />
	<span asp-validation-for="FirstName"></span>
</div>
<div class="form-group w-50">
	<label class="col-form-label" asp-for="LastName">LastName</label>
	<input readonly class="form-control" asp-for="LastName" />
	<span asp-validation-for="LastName"></span>
</div>
<div class="form-group w-50">
	<label class="col-form-label" asp-for="Email">Email</label>
	<input readonly class="form-control" asp-for="Email" />
	<span asp-validation-for="Email"></span>
</div>

<div class="form-group w-50">
	<label class="col-form-label" asp-for="Roles">Roles</label>
	<input readonly class="form-control" asp-for="Roles" />
	<span asp-validation-for="Roles"></span>
</div>

