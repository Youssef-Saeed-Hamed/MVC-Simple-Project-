@model DepartmentVM

<div class="form-group w-50">
    <label class="col-form-label" asp-for="Code">Code</label>
    <input readonly class="form-control" asp-for="Code" />
</div>
<div class="form-group w-50">
    <label class="col-form-label" asp-for="Name">Name</label>
    <input readonly class="form-control" asp-for="Name" />
</div>
<div class="form-group w-50">
    <label class="col-form-label" asp-for="CreatedDate">CreatedDate</label>
    <input readonly class="form-control" asp-for="CreatedDate" />
</div>
<div class="form-group w-50 mb-3">
    <label class="col-form-label" asp-for="Description">Description</label>
    <textarea readonly class="form-control" asp-for="Description"></textarea>
</div>