
@model DepartmentVM
<div class="form-group w-50">
    <label class="col-form-label" asp-for="Code">Code</label>
    <input class="form-control" asp-for="Code" />
    <span asp-validation-for="Code" class="text-danger"></span>
</div>
<div class="form-group w-50">
    <label class="col-form-label" asp-for="Name">Name</label>
    <input class="form-control" asp-for="Name" />
    <span asp-validation-for="Name" class="text-danger"></span>
</div>
<div class="form-group w-50">
    <label class="col-form-label" asp-for="CreatedDate">CreatedDate</label>
    <input class="form-control" asp-for="CreatedDate" />
    <span asp-validation-for="CreatedDate" class="text-danger"></span>
</div>
<div class="form-group w-50">
    <label class="col-form-label" asp-for="Description">Description</label>
    <textarea class="form-control" asp-for="Description"></textarea>
    <span asp-validation-for="Description" class="text-danger"></span>
</div>